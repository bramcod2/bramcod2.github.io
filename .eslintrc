{
  "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
  ],
  "plugins": [
      "jsx-a11y",
      "prettier"
  ],
  "rules": {
    "no-unused-vars": "off", // turn on later
    "semi": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
        "error", {
        "semi": false
      }
    ],
    "react/destructuring-assignment": "off",
    "react/prop-types": "off"
  }
}